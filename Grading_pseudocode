START
  PROMPT user to input score
  CONVERT input to integer

  IF score >= 70 AND score <= 100
    PRINT "Grade: A"
  ELSE IF score >= 60 AND score <= 69
    PRINT "Grade: B"
  ELSE IF score >= 50 AND score <= 59
    PRINT "Grade: C"
  ELSE IF score >= 45 AND score <= 49
    PRINT "Grade: D"
  ELSE IF score >= 40 AND score <= 44
    PRINT "Grade: E"
  ELSE IF score < 40
    PRINT "Grade: F (Fail)"
  ELSE
    PRINT "Invalid Score"
END
